<view class='container'>
  <include src="../../footer.wxml"/>
  <view class='header'>
    <text>注册页面</text>
  </view>
  <form bindsubmit="register">
    <view class='section'>
      <text>手机号码：</text>
      <input type='number' placeholder='请输入手机号码' name="username" value='1635050238'/>
    </view>
    <view class='section'>
      <text>个人密码:</text>
      <input password='true' placeholder='请输入密码' name="password" value='123456'/>
    </view>
     <view class='section'>
      <text>再次输入密码:</text>
      <input password='true' placeholder='请输入密码' name="password1" value='123456'/>
    </view>
    <view class='section'>
      <text>孩子名称:</text>
      <input type='number' placeholder='请输入孩子名称' name="childname" value='123456'/>
    </view>
    <view class='button'>
      <button type='primary' form-type='submit'>注册</button>
    </view>
  </form>
</view>